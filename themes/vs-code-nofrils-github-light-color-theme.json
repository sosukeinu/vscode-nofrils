  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#24292e"
    },
    "variable.constant": {
      "foreground": "#24292e"
    },
    "variable.defaultLibrary": {
      "foreground": "#24292e"
    }
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6a737d"
      }
    },
    {
      "name": "All other syntax",
      "scope": "source",
      "settings": {
        "foreground": "#24292e"
      }
    },
    {
      "name": "Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#d73a49"
      }
    },
    {
      "name": "Broken",
      "scope": "invalid.broken",
      "settings": {
        "foreground": "#d73a49"
      }
    },
    {
      "name": "Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#d73a49"
      }
    },
    {
      "name": "Unimplemented",
      "scope": "invalid.unimplemented",
      "settings": {
        "foreground": "#d73a49"
      }
    }
  ]
