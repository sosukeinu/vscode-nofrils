  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#abb2bf"
    },
    "variable.constant": {
      "foreground": "#abb2bf"
    },
    "variable.defaultLibrary": {
      "foreground": "#abb2bf"
    }
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5c6370"
      }
    },
    {
      "name": "All other syntax",
      "scope": "source",
      "settings": {
        "foreground": "#abb2bf"
      }
    },
    {
      "name": "Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Broken",
      "scope": "invalid.broken",
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Unimplemented",
      "scope": "invalid.unimplemented",
      "settings": {
        "foreground": "#e06c75"
      }
    }
  ]
