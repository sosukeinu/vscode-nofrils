  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#383a42"
    },
    "variable.constant": {
      "foreground": "#383a42"
    },
    "variable.defaultLibrary": {
      "foreground": "#383a42"
    }
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a0a1a7"
      }
    },
    {
      "name": "All other syntax",
      "scope": "source",
      "settings": {
        "foreground": "#383a42"
      }
    },
    {
      "name": "Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#e45649"
      }
    },
    {
      "name": "Broken",
      "scope": "invalid.broken",
      "settings": {
        "foreground": "#e45649"
      }
    },
    {
      "name": "Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#e45649"
      }
    },
    {
      "name": "Unimplemented",
      "scope": "invalid.unimplemented",
      "settings": {
        "foreground": "#e45649"
      }
    }
  ]
