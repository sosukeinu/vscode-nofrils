  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#657b83"
    },
    "variable.constant": {
      "foreground": "#657b83"
    },
    "variable.defaultLibrary": {
      "foreground": "#657b83"
    }
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#93a1a1"
      }
    },
    {
      "name": "All other syntax",
      "scope": "source",
      "settings": {
        "foreground": "#657b83"
      }
    },
    {
      "name": "Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "Broken",
      "scope": "invalid.broken",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "Unimplemented",
      "scope": "invalid.unimplemented",
      "settings": {
        "foreground": "#dc322f"
      }
    }
  ]
